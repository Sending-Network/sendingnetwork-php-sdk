{
    "name": "sending-network/sdn-php-sdk",
    "type": "library",
    "description": "PHP SDK for interacting with Sending-Network",
    "keywords": [
        "SDN",
        "Client",
        "SDK",
        "PHP"
    ],
    "homepage": "https://github.com/Sending-Network/sendingnetwork-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "James Cooper",
            "email": "jamescooper@sending.network",
            "homepage": "https://github.com/Sending-Network/sendingnetwork-php-sdk",
            "role": "Developer"
        }
    ],
    "replace": {
        "sending-network/sdk-php-sdk": "self.version"
    },
    "require": {
        "php": "^8",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7",
        "kornrunner/keccak": "^1.1",
        "rappasoft/laravel-helpers": "^1.0",
        "simplito/elliptic-php": "^1.0",
        "ext-readline": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3"
    },
    "autoload": {
        "psr-4": {
            "SdnSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SdnSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
